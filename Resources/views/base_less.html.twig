{% extends 'opwocoBootstrapBundle::base.html.twig' %}

{% block head_style %}
    {# Override this block to add your own files! #}
    {% stylesheets
        '@opwocoBootstrapBundle/Resources/public/less/opwocobootstrapbundle.less'
    %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
    {% endstylesheets %}
    {# To use this without less use the base_css.html.twig template as your base
     # Be sure you understand whats going on: have a look into
     # https://github.com/opwoco/BootstrapBundle/blob/master/Resources/doc/css-vs-less.md
     #}
{% endblock head_style %}

{% block foot_script_assetic %}
    {% javascripts
    '@opwocoBootstrapBundle/Resources/public/bootstrap/js/tooltip.js'
    '@opwocoBootstrapBundle/Resources/public/bootstrap/js/*.js'
    '@opwocoBootstrapBundle/Resources/public/js/opwocobootstrap-collection.js'
    '@opwocoBootstrapBundle/Resources/public/js/opwocobootstrap-subnav.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock foot_script_assetic %}
